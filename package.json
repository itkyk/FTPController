{
  "name": "@itkyk/ftp-controller",
  "version": "1.0.0-beta.0.4",
  "main": "bin/index.cjs",
  "types": "types/index.d.ts",
  "module": "bin/index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itkyk/FTPController.git"
  },
  "bin": {
    "ftp": "./bin/cli/index.cjs"
  },
  "napi": {
    "name": "ftp-controller",
    "triples": {
      "additional": [
        "aarch64-apple-darwin"
      ]
    }
  },
  "exports": {
    ".": {
      "require": "./bin/index.cjs",
      "import": "./bin/index.mjs",
      "types": "./types/index.d.ts"
    },
    "./core": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.17.0",
    "@types/interpret": "^1.1.3",
    "@types/node": "^20.10.5",
    "@types/rechoir": "^0.6.4",
    "esbuild": "^0.19.10",
    "esbuild-node-externals": "^1.12.0",
    "esbuild-plugin-ignore": "^1.1.1",
    "glob": "^10.3.10",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:lib": "napi build --platform --release ./lib",
    "build:src": "tsc --outDir dist && node -r esbuild-register ./build && tsc --declaration --emitDeclarationOnly --outDir types",
    "build:all": "npm run build:lib && npm run build:src",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "universal": "napi universal",
    "version": "napi version",
    "test": "node ./bin/cli/index.js --init"
  },
  "packageManager": "yarn@4.0.2",
  "description": "ftp uploader core code",
  "dependencies": {
    "commander": "^11.1.0",
    "esbuild-register": "^3.5.0"
  }
}
