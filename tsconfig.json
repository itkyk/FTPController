{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
     "rootDir": "./ts-src/",
     "declaration": true,
     "outDir": "./types/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["ts-src", "ts-src/rust"],
  "exclude": ["bin"]
}
